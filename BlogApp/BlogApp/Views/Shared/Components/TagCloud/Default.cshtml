@model List<Tag>
@inject IViewLocalizer localizer

<div>
    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">@localizer["Etiketler"]</div>
</div>
<div class="clearfix"></div>
<div class="fh5co_tags_all">
    @foreach(Tag t in Model)
    {
         <a  asp-controller="blog" asp-action="getbytag" asp-route-tagslug="@t.SlugUri" class="fh5co_tagg">@t.Title</a>
        
    }
    
</div>